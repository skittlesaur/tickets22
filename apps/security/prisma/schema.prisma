generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// registered user
model User {
  id                String           @id @default(cuid()) @map("_id")
  email             String           @unique
  password          String
  name              String?
  phone             String?
  verified          Boolean          @default(false)
  verificationToken String
  createdAt         DateTime         @default(now())
  //ReservedTicket    ReservedTicket[]
}
